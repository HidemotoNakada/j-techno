    "import re\n",
    "sample = \"\"\"# \n",
    "# hideall\n",
    "test2\n",
    "\"\"\"\n",
    "\n",
    "def compile(patterns):\n",
    "    if not patterns:\n",
    "        return None\n",
    "    return re.compile('|'.join('(?:%s)' % pattern\n",
    "                    for pattern in patterns), re.M)\n",
    "\n",
    "pat = compile(['^# hide$'])\n",
    "\n",
    "pat.search(sample)\n"
   ]
